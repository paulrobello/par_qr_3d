{
  "include": [
    "src/**/*.py"
  ],
  "exclude": [
    "**/node_modules",
    "**/__pycache__",
    "**/output",
    "**/build",
    "src/par_qr_3d-main-branch/**"
  ],
  "ignore": [
    "**/.venv"
  ],
  "defineConstant": {
    "DEBUG": true
  },
  "venvPath": ".",
  "venv": ".venv",
  "reportMissingImports": true,
  "reportMissingTypeStubs": false,
  "pythonVersion": "3.12",
  "typeCheckingMode": "basic"
}
